        <Label Content="" Name="playing_link" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="344"/>
        <Label Content="" Name="playing_user"  HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" Width="344"/>
        <Button Content="Suivant" Name="next" HorizontalAlignment="Left" Margin="65,86,0,0" VerticalAlignment="Top" Width="75" Click="next_Click"/>
        <Button Content="Bannir utilisateur" Name="playing_ban" HorizontalAlignment="Left" Margin="197,86,0,0" VerticalAlignment="Top" Width="110" Click="playing_ban_Click"/>

    </Grid>
</Window>
